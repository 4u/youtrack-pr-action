{
  "cacheDirectory": "node_modules/.cache/jest",
  "collectCoverageFrom": ["src/**/*.ts"],
  "moduleDirectories": ["node_modules"],
  "moduleFileExtensions": ["ts", "js", "json"],
  "preset": "ts-jest",
  "reporters": ["default"],
  "testPathIgnorePatterns": ["<rootDir>/out/"],
  "transform": {"^.+\\.ts$": "ts-jest"},
  "testEnvironment": "node"
}
